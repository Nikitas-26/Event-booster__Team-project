{{#each this}}
<div class="modal modal-js is-hidden">
    <button class="modal-close modal-close-js">Close</button>
    <div>
        <img src="{{events.images.url}}" alt="{{events.images.attribution}}">
        <div class="modal-content modal-content-js">
            <h2>INFO</h2>
            <p>{{events.description}}</p>
            <h2>WHEN</h2>
            <p>{{events.dates.start.localDate}}</p>
            <h2>WHO</h2>
            <p>{{events.name}}</p>
            <h2>PRICES</h2>
            <p>{{events.priceRanges.min}}</p>
            <button>BUY TICKET</button>
            <p>{{events.priceRanges.max}}</p>
            <button>BUY TICKET</button>
            <button>MORE FROM THIS AUTHOR</button>
        </div>
    </div>

</div>
{{/each}}